    "commitlint",
    "cli"
  ],
  "author": {
    "name": "Mario Nebl",
    "email": "hello@herebecode.com"
  },
  "license": "MIT",
  "dependencies": {
    "@commitlint/cli": "^7.0.0",
    "read-pkg": "4.0.1",
    "resolve-pkg": "1.0.0"
